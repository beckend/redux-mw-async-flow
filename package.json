{
  "name": "redux-mw-async-flow",
  "version": "1.0.0",
  "description": "redux async handling for any actions without enforcing API client implementation",
  "homepage": "https://github.com/beckend/redux-mw-async-flow#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beckend/redux-mw-async-flow.git"
  },
  "bugs": {
    "url": "https://github.com/beckend/redux-mw-async-flow/issues"
  },
  "files": [
    "build-es5",
    "build-es2015",
    "build-es2017"
  ],
  "main": "./build-es5/index.js",
  "jsnext:main": "./build-es2015/index.js",
  "types": "./build-es5/index.d.ts",
  "scripts": {
    "prepare-before": "gulp",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage --no-cache",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "docker-test": "docker-compose -f docker-compose.test.yml up"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(spec))\\.(ts|tsx)$",
    "collectCoverage": false,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "lcovonly",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./src/tsconfig-test.json"
      }
    },
    "mapCoverage": true
  },
  "keywords": [
    "typescript",
    "react",
    "redux",
    "middleware",
    "middlewares",
    "promise",
    "promises",
    "optimistic update",
    "optimistic updates",
    "async"
  ],
  "author": "Linken Dinh",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.5.18",
    "@types/debug": "^0.0.30",
    "@types/fs-extra": "^4.0.5",
    "@types/globby": "^6.1.0",
    "@types/gulp": "^4.0.5",
    "@types/jest": "^21.1.8",
    "@types/lodash": "^4.14.88",
    "@types/lodash.clonedeep": "^4.5.3",
    "@types/lodash.get": "^4.4.3",
    "@types/lodash.merge": "^4.6.3",
    "@types/lodash.set": "^4.3.3",
    "@types/react": "^16.0.28",
    "@types/react-dom": "^16.0.3",
    "@types/redux-actions": "^2.2.3",
    "@types/redux-mock-store": "0.0.12",
    "cmd-spawn": "^1.4.0",
    "coveralls": "^3.0.0",
    "debug": "^3.1.0",
    "fs-extra": "^4.0.3",
    "globby": "^7.1.1",
    "google-closure-compiler-js": "^20171203.0.0",
    "gulp": "gulpjs/gulp.git#4.0",
    "gulp-debug": "^3.1.0",
    "gulp-run": "^1.7.1",
    "gulp-sourcemap": "^1.0.1",
    "gulp-tslint": "^8.1.2",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-mock-store": "^1.3.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.0.1",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2"
  },
  "peerDependencies": {
    "redux": "3.*"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.0",
    "lodash.set": "^4.3.2",
    "rxjs": "^5.4.3",
    "tslib": "^1.7.1",
    "uniqueid": "^1.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
